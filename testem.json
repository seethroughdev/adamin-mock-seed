{
  "launch_in_dev": [
    "phantomJS"
  ],
  "src_files": [
    "app/js/*.coffee",
    "tests/spec/*.coffee"
  ],
  "serve_files": [
    "tests/lib/*.js",
    "components/jquery/jquery.min.js",
    "components/underscore/underscore-min.js",
    "components/Adamin-Clone/dist/adamin_clone.min.js",
    "dist/js/*.js",
    "tests/spec/*.js"
  ],
  "before_tests": "coffee -c tests/*",
  "on_exit": "rm tests/spec/*.js",
  "framework": "jasmine"
}